@model IEnumerable<Project_try3.Models.Products>


<table class="table">
    <tr>
        <th>

        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stock)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.UnitPrice)
        </th>



        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @*@Html.DisplayFor(modelItem => item.Photo)*@
                <img src="@Url.Action("GetImage","Products", new { @id = item.SN })" width="100">
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stock)/@Html.DisplayFor(modelItem => item.Unit)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.UnitPrice)
            </td>

            <td>
                <input id="Text1" type="text" />
            </td>
 

            <td>
                @*@Html.ActionLink("Edit", "Edit", new { id=item.SN }) |
                    @Html.ActionLink("Details", "Details", new { id=item.SN }) |
                    @Html.ActionLink("Delete", "Delete", new { id=item.SN })*@
            </td>
        </tr>
    }

</table>
