<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Payed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GroupBuying.Title)
        </th>
